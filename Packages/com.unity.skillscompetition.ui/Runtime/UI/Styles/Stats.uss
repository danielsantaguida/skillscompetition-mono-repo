.stats-screen {
    width:100%;
    height:10%;
    justify-content: space-between;
    flex-direction: row;
    align-content: center;
}

.shot-clock {
    font-size: 400%;
    width: 33%;
    -unity-text-align: middle-center;
    color: red;
}

.points-label {
    font-size: 400%;
    width: 33%;
    -unity-text-align: middle-right;
}

.end-screen {
    width: 100%;
    height: 100%;
}